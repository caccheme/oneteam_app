
<h1>Responses to request <%= @request.id %>:</h1>
<h4>"<%= @request.description %>"</h4>


<table>
  <tr>
    <th>Request</th>
    <th>Employee Name</th>
    <th>Employee Profile</th>
    <th>Comment</th>
    
  </tr>

    <% @responses.each do |response| %>
      <tr >
       <td><%= response.request_id %></td>
       <td><%= response.employee_name %></td>
       <td><%= link_to 'Profile', employee_path(response.employee_id) %></td>
       <td><%= response.comment %></td>    
         <td><%= link_to 'Select This Developer', new_response_commission_path(response) %></td>      
      </tr>
      <% end %>
      </table>
<%= paginate @responses %>    
<div>    
<%= link_to 'Back to Requests', requests_path %>
</div>